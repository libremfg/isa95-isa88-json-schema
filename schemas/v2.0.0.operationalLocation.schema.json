    "OperationalLocation": {
      "$ref": "#/$defs/OperationalLocationType",
      "description": "A logical or physical location where a material lot, material sublot, equipment, physical asset or person\nis located or expected to be located shall be presented as an operational location. Operational locations\nmay be made up of other operational locations."
    },
    "OperationalLocationClass": {
      "$ref": "#/$defs/OperationalLocationClassType",
      "description": "A representation of a grouping of operational locations with similar characteristics for a definite purpose\nsuch as manufacturing operations definition, scheduling, capability and performance shall be presented as an\noperational location class. Any operational location may be a member of zero or more operational location classes."
    },
